		value="30000" />
	<property name="datasource.minEvictableIdleTimeMillis" value="180000" />
	<property name="datasource.defaultAutoCommit" value="false" />
	<property name="datasource.initialSize" value="25" />
	<property name="datasource.jmxEnabled" value="true" />
