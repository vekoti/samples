<?xml version="1.0" encoding="UTF-8"?>
<config>
	<property name="datasource.driverClassName" value="oracle.jdbc.OracleDriver" />
	<property name="datasource.url"
		value="" />
	<property name="datasource.username" value="" />
	<property name="datasource.password" value="" />
	<property name="datasource.closeMethod" value="close" />
	<property name="datasource.maxIdle" value="10" />
	<property name="datasource.maxAge" value="3000" />
	<property name="datasource.type" value="javax.sql.DataSource" />
	<property name="datasource.maxActive" value="100" />
	<property name="datasource.minIdle" value="5" />
	<property name="datasource.maxWait" value="600000" />
