	<property name="datasource.testOnReturn" value="true" />
	<property name="datasource.testOnConnect" value="true" />
	<property name="datasource.testWhileIdle" value="true" />
	<property name="datasource.validationQuery" value="select 1 from dual" />
	<property name="datasource.validationInterval" value="60000" />
	<property name="datasource.removeAbandonedTimeout" value="1800" />
	<property name="datasource.accessToUnderlyingConnectionAllowed"
		value="true" />

	<property name="model.package" value="com.cisco.ccw.quote.mongodb.model" />
